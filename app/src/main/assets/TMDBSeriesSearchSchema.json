{
	"$id": "https://example.com/define.schema.json",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"description": "TMDB Series Search Results JSON Schema",
	"type": "object",
	"mapping": "net.forestany.mediacollection.search.data.SeriesSearchResults",
	"properties": {
		"page": {
			"type": "integer",
			"mapping": "Page"
		},
		"results": {
			"type": "array",
			"mapping": "Results:java.util.List",
			"items": {
				"SearchResultRef": {
					"type": "object",
					"mapping": "net.forestany.mediacollection.search.data.SeriesSearchResult",
					"properties": {
						"adult": {
							"type": "boolean",
							"mapping": "Adult"
						},
						"backdrop_path": {
							"type": "string",
							"mapping": "BackdropPath"
						},
						"genre_ids": {
							"type": "array",
							"mapping": "GenreIds",
							"items": {
								"type": "integer[]"
							}
						},
						"id": {
							"type": "integer",
							"mapping": "Id"
						},
						"origin_country": {
							"type": "array",
							"mapping": "OriginCountry",
							"items": {
								"type": "string[]"
							}
						},
						"original_language": {
							"type": "string",
							"mapping": "OriginalLanguage"
						},
						"original_name": {
							"type": "string",
							"mapping": "OriginalName"
						},
						"overview": {
							"type": "string",
							"mapping": "Overview"
						},
						"popularity": {
							"type": "number",
							"mapping": "Popularity"
						},
						"poster_path": {
							"type": "string",
							"mapping": "PosterPath"
						},
						"first_air_date": {
							"type": "string",
							"mapping": "FirstAirDate"
						},
						"name": {
							"type": "string",
							"mapping": "Name"
						},
						"vote_average": {
							"type": "number",
							"mapping": "VoteAverage"
						},
						"vote_count": {
							"type": "integer",
							"mapping": "VoteCount"
						}
					},
					"required": [
						"id",
						"original_name",
						"name"
					]
				}
			},
			"minItems": 0
		},
		"total_pages": {
			"type": "integer",
			"mapping": "TotalPages"
		},
		"total_results": {
			"type": "integer",
			"mapping": "TotalResults"
		}
	},
	"required": [
		"results",
		"total_results"
	]
}